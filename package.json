{
  "name": "autocommiterr",
  "displayName": "autocommiter copilot",
  "description": "auto commit messages using github",
  "repository": "https://github.com/nathfavour/autocommiter",
  "publisher": "nathfavour",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocommiter.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "autocommiter.generateMessage",
        "title": "Autocommit"
      }
    ],
    "menus": {
      "view/title": [
        {
          "when": "view == scm",
          "command": "autocommiter.generateMessage",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Autocommiter",
      "properties": {
        "autocommiter.updateGitignore": {
          "type": "boolean",
          "default": false,
          "description": "Automatically update .gitignore to protect sensitive files (.env*, docx/) and nested repositories"
        },
        "autocommiter.gitignorePatterns": {
          "type": "array",
          "default": [
            "*.env*",
            ".env*",
            "docx/",
            ".docx/"
          ],
          "description": "Patterns to add to .gitignore when updateGitignore is enabled",
          "items": {
            "type": "string"
          }
        },
        "autocommiter.selectedModel": {
          "type": "string",
          "default": "gpt-4o-mini",
          "description": "The default AI model to use for generating commit messages. Examples: gpt-4o-mini, gpt-4o, Meta-Llama-3.1-70B-Instruct, Mistral-large"
        },
        "autocommiter.autoUpdateModels": {
          "type": "boolean",
          "default": true,
          "description": "Automatically refresh the list of available models from the API when using model selection"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "license": "MIT"
}